{
	"env": {
		"es6": true
	},
	// 0=off, 1=warning (no fail), 2=error (exit code of 1)
	"rules": {
		"brace-style": [2, "stroustrup", { "allowSingleLine": false }],
		"default-case": 2,
		"indent": [2, "tab"],
		"no-catch-shadow": 2,
		"no-eq-null": 2,
		"no-floating-decimal": 2,
		"no-inline-comments": 0,
		"no-nested-ternary": 2,
		"no-self-compare": 2,
		"no-throw-literal": 2,
		"no-undefined": 0,
		"no-void": 2,
		"quotes": [1, "double"],
		"space-after-keywords": 2,
		"wrap-iife": [2, "outside"],
		"react/display-name": 2,
		"react/prop-types": 2
	},
	"ecmaFeatures": {
		"modules": true,
		"jsx": true
	},
	"plugins": [
		"react"
	]
}
